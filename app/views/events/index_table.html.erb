<table class="table-striped table-bordered table-condensed">
	<tr>
		<th><%= Event.column_names.second.capitalize %></th>
		<th><%= Event.column_names.third.capitalize %></th>
		<th><%= Event.column_names.fourth.capitalize %></th>
		<th>Event Duration</th>
		<th>Sponsor <%= Company.column_names[4].capitalize %></th>
	</tr>

<% @events.each do |event| %>
	<tr>
		<td class><%= link_to event.title, event_path(event) %></td>
		<td><%= event.location %></td>
		<td><%= event.date.strftime("%m-%d-%Y") %></td>
		<td><%= event.date.strftime("%I:%M %P") %> - <%= event.end_date.strftime("%I:%M %P") %></td>
		<td><%= image_tag(Company.find(event.company_id).logo, size: "125", class: "media") %></td>
	</tr>
<% end %>
</table>
<br/>
<p class="links"><%= link_to 'Back', :back %></p>

